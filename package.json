{
  "name": "este",
  "private": true,
  "engines": {
    "node": "10.x.x"
  },
  "license": "MIT",
  "scripts": {
    "clean-babel-cache": "rimraf 'node_modules/.cache/babel-loader'",
    "babel": "node -r esm -r flow-remove-types/register ./scripts/babel",
    "dev:android": "yarn clean-babel-cache && yarn babel native && START_SCRIPT=start:native && yarn react-native run-android",
    "dev:ios": "yarn clean-babel-cache && yarn babel native && START_SCRIPT=start:native && yarn react-native run-ios",
    "dev:api": "nodemon --config nodemon.api.json",
    "dev:web": "nodemon --config nodemon.web.json",
    "dev:relay:watch": "yarn codegen:relay --watch",
    "dev:playground": "graphql playground --port 5000 --server-only",
    "dev": "yarn clean-babel-cache && yarn babel web && run-p dev:api dev:web dev:relay:watch dev:playground",
    "build": "next build",
    "production:api": "cross-env NODE_ENV=production node -r esm -r flow-remove-types/register server/api/index",
    "production:web": "cross-env NODE_ENV=production node -r esm -r flow-remove-types/register server/web/index",
    "production": "yarn clean-babel-cache && env-cmd .env npm-run-all build --parallel production:api production:web",
    "start:native": "node node_modules/react-native/local-cli/cli.js start",
    "start": "yarn $START_SCRIPT",
    "prisma": "prisma",
    "deploy:db": "rimraf 'database/schema.graphql' && yarn prisma deploy",
    "deploy:api": "now -e START_SCRIPT=production:api --dotenv .env.production",
    "deploy:web": "now -e START_SCRIPT=production:web --dotenv .env.production",
    "codegen:schema": "graphql get-schema -p api -o server/api/schema.graphql --all false",
    "codegen:relay:ci": "yarn codegen:relay --watchman false",
    "codegen:relay": "relay-compiler --src . --include 'client/**' 'components/**' 'mutations/**' 'pages/**' --schema server/api/schema.graphql --noFutureProofEnums true",
    "codegen:graphql:fix": "node -r esm -r flow-remove-types/register ./scripts/fixCodegen",
    "codegen:graphql": "graphql codegen && yarn codegen:graphql:fix",
    "codegen": "yarn codegen:schema && yarn codegen:relay && yarn codegen:graphql",
    "messages:extract": "yarn clean-babel-cache && rimraf 'lang/.messages' && next build && node -r esm -r flow-remove-types/register ./scripts/messages/extract",
    "messages:clean": "node -r esm -r flow-remove-types/register ./scripts/messages/clean",
    "messages:missing": "node -r esm -r flow-remove-types/register ./scripts/messages/missing",
    "messages": "run-s messages:extract messages:clean messages:missing",
    "favicon": "node -r esm -r flow-remove-types/register ./scripts/favicon",
    "flow": "flow",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "format": "prettier --write '**/*.js'",
    "eslint": "eslint browser client components mutations pages screens scripts server test themes validations *.js",
    "test": "run-s codegen:relay:ci eslint flow jest messages",
    "deps": "rimraf 'node_modules' 'yarn.lock' 'flow-typed' && yarn && flow-typed install",
    "env": "node -r esm -r flow-remove-types/register ./scripts/env",
    "analyze:bundles": "cross-env ANALYZE=BUNDLES next build",
    "analyze:size": "cross-env ANALYZE=SIZE next build && cat '.next/stats.txt'",
    "docker:up": "cd database && docker-compose up -d && cd ../",
    "docker:down": "cd database && docker-compose down && cd ../"
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-react-native-web": "^0.8.8",
    "babel-plugin-transform-inline-environment-variables": "^0.4.0",
    "bcryptjs": "^2.4.3",
    "color": "^3.0.0",
    "cookie": "^0.3.1",
    "cross-env": "^5.0.1",
    "diacritics-map": "^0.1.0",
    "esm": "^3.0.71",
    "flow-remove-types": "^1.2.3",
    "glob": "^7.1.2",
    "graphql": "^0.13.0",
    "graphql-shield": "^3.1.2",
    "graphql-yoga": "^1.14.5",
    "gravatar": "^1.6.0",
    "immutable": "^3.8.2",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.10",
    "next": "^6.1.0",
    "nprogress": "^0.2.0",
    "open-color": "^1.6.3",
    "prisma-binding": "^2.0.2",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "ramda": "^0.25.0",
    "react": "16.5.0",
    "react-art": "16.4.2",
    "react-dom": "16.4.2",
    "react-intl": "^2.3.0",
    "react-native": "0.57.0",
    "react-native-web": "^0.8.0",
    "react-relay": "^1.6.0",
    "relay-runtime": "^1.6.0",
    "slate": "^0.40.0",
    "slate-react": "^0.18.0",
    "url": "^0.11.0",
    "validator": "^10.1.0",
    "webpack-bundle-analyzer": "^3.0.1",
    "webpack-bundle-size-analyzer": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-proposal-optional-chaining": "7.0.0-beta.42",
    "@babel/preset-flow": "7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-relay": "^1.6.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^4.0.0",
    "dotenv": "^6.0.0",
    "env-cmd": "^8.0.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-relay": "^0.0.28",
    "flow-bin": "^0.80.0",
    "flow-typed": "^2.1.5",
    "graphql-cli": "^2.13.0",
    "jest": "^23.1.0",
    "nodemon": "^1.15.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.5.1",
    "prisma": "^1.2.4",
    "react-test-renderer": "16.4.2",
    "relay-compiler": "^1.6.0",
    "rimraf": "^2.4.5"
  }
}
